{
  "watch": ["server.js", "config", "controllers", "routes", "services"],
  "ignore": [
    "data/*",
    "data/**/*",
    ".wwebjs_auth",
    ".wwebjs_auth/**/*",
    ".wwebjs_cache",
    ".wwebjs_cache/**/*",
    "node_modules",
    "node_modules/**/*",
    ".env",
    "*.log",
    ".git",
    ".git/**/*"
  ],
  "ext": "js,mjs,cjs,json",
  "delay": 500,
  "verbose": false
}
